{
  "name": "pokemon-app",
  "version": "1.0.0",
  "description": "This web app is showing First Generation Pokemons with using pokemon API",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack-dev-server --open ",
    "start": "webpack-dev-server --config webpack.config.prod.js",
    "build": "webpack --config webpack.config.prod.js",
    "build:prod": "npm run clean && webpack --config webpack.config.prod.js",
    "clean": "rimraf ./dist/*",
    "lint": "eslint src",
    "precommit": "lint-staged",
    "test": "jest --watchAll",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage --colors"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Berat Tetik <beratetik@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@types/jest": "^25.1.4",
    "assets-webpack-plugin": "^3.9.10",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react-test-renderer": "^16.13.0",
    "redux-mock-store": "^1.5.4",
    "style-loader": "^1.1.3",
    "url-loader": "^3.0.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-image-fallback": "^8.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.42.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
